{
    "description": "switch profiles",
    "manipulators": [
        {
            "type": "basic",
            "from": {
                "key_code": "hyphen",
                "modifiers": {
                    "optional": [
                        "shift",
                        "command",
                        "option",
                        "caps_lock"
                    ]
                }
            },
            "to": [
                {
                    "shell_command": "'/Library/Application Support/org.pqrs/Karabiner-Elements/bin/karabiner_cli' --select-profile 'No changes profile'"
                }
            ]
        }
    ]
}